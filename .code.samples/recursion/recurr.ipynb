    "def fib (n):\n",
    "    return fibo (n, 0, 1, 0)\n",
    "\n",
    "def fibo (i, prev, curr, buf):\n",
    "    if i == 0:\n",
    "        return buf\n",
    "    else:\n",
    "        return fibo (i-1, curr, prev+curr, prev+curr)\n",
    "\n",
    "aa = fib(21)\n",
    "print(aa)"
   ]
